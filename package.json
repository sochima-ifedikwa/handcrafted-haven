{
  "name": "handcrafted-haven",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "auth:secret": "node scripts/generate-auth-secret.cjs",
    "prisma:generate": "dotenv -e .env.development.local -- prisma generate",
    "prisma:migrate": "dotenv -e .env.development.local -- prisma migrate dev",
    "prisma:migrate:deploy": "dotenv -e .env.development.local -- prisma migrate deploy",
    "prisma:migrate:status": "dotenv -e .env.development.local -- prisma migrate status",
    "prisma:studio": "dotenv -e .env.development.local -- prisma studio",
    "db:seed": "dotenv -e .env.development.local -- node prisma/seed.mjs",
    "postinstall": "dotenv -e .env.development.local -- prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "prisma": "^6.16.2",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "typescript": "^5"
  }
}
